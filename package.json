{
  "name": "spicetify-hide-podcasts",
  "version": "3.1.0",
  "description": "Spicetify extension to hide podcasts.",
  "main": "hidePodcasts.js",
  "homepage": "https://github.com/theRealPadster/spicetify-hide-podcasts",
  "repository": {
    "type": "git",
    "url": "git@github.com:theRealPadster/spicetify-hide-podcasts.git"
  },
  "bugs": {
    "url": "https://github.com/theRealPadster/spicetify-hide-podcasts/issues"
  },
  "author": "theRealPadster",
  "license": "GPL-3.0",
  "packageManager": "pnpm@9.0.4",
  "scripts": {
    "build": "spicetify-creator",
    "build:local": "spicetify-creator --out=. --minify",
    "watch": "spicetify-creator --watch",
    "lint": "eslint --fix src",
    "lint:ci": "eslint src",
    "type-check": "tsc --noEmit",
    "prepare": "husky install",
    "update-types": "curl -s -o src/types/spicetify.d.ts https://raw.githubusercontent.com/spicetify/spicetify-cli/master/globals.d.ts"
  },
  "engines": {
    "yarn": "please-use-pnpm",
    "npm": "please-use-pnpm",
    "pnpm": ">=8",
    "node": ">=20"
  },
  "devDependencies": {
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.37.1",
    "husky": "^9.1.4",
    "i18next": "^25.3.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "react-i18next": "^15.5.1",
    "spicetify-creator": "^1.0.17",
    "typescript": "^5.8.2"
  },
  "private": true
}
