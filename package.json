  "private": true,
  "devDependencies": {
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.53.0",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^8.0.3",
    "i18next": "^23.6.0",
    "i18next-browser-languagedetector": "^7.1.0",
    "react-i18next": "^13.3.1",
    "spicetify-creator": "^1.0.16",
    "typescript": "^5.2.2"
  }
